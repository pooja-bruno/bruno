meta {
  name: clear
  type: http
  seq: 6
}

get {
  url: {{host}}/ping
  body: none
  auth: inherit
}

tests {
  const jar = bru.cookies.jar()
  
  jar.clear(function(error) {
    if(error) {
      console.error("Clear cookie jar error:", error)
      throw new Error(`Failed to clear cookie jar: ${error.message || error}`)
    }
    
    test("should successfully clear entire cookie jar", function() {
      expect(error).to.be.null;
      console.log("Cookie jar cleared successfully");
    });
  })
}

settings {
  encodeUrl: true
}
